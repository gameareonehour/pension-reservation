{
  "name": "pension-reservation-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier src/app '**/*.ts' '**/*.tsx' '**/**/*.ts' '**/**/*.tsx' --write",
