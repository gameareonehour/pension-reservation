    "style-dictionary": "style-dictionary build --config style-dictionary.json"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "openapi-typescript": "^6.7.3",
    "prettier": "^3.1.1",
    "style-dictionary": "^3.9.1",
    "token-transformer": "^0.0.33",
    "typescript": "^5"
  }
}
